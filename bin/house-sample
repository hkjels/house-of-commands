#!/usr/bin/env bash

cmd=$(basename $0)

#
# Output usage
#

usage() {
  cat << EOT

  Usage: $cmd [options]

  Options:

    -h, --help          output usage information

EOT
}

#
# Parse arguments
#

while test $# -ne 0; do
  arg=$1; shift
  case $arg in
    __description)
      echo "sample interface for a house"
      exit 0
      ;;
    -h|--help)
      usage
      exit 0
      ;;
  esac
done

